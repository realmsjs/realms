{
  "name": "@realmsland/core",
  "version": "1.0.0",
  "description": "realmsland core that interacts with and manages all other packages.",
  "main": "./cjs/index.cjs",
  "types": "./dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": {
        "require": "./cjs/*.cjs"
      }
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oasisjs/realmsland.git"
  },
  "keywords": [
    "core"
  ],
  "author": "Miia Egnisa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oasisjs/realmsland/issues"
  },
  "homepage": "https://github.com/oasisjs/realmsland",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packemon": {
    "format": "cjs",
    "platform": "node",
    "support": "experimental"
  },
  "type": "commonjs",
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "src/**/*.{ts,tsx,json}"
  ],
  "dependencies": {
    "@realmsland/api-references": "^1.0.0",
    "@realmsland/rest": "^1.0.0",
    "@realmsland/ws": "^1.0.0"
  }
}
